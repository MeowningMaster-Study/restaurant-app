{
    "compilerOptions": {
        "target": "ESNext",
        "module": "ESNext",
        "moduleResolution": "bundler",
        "allowSyntheticDefaultImports": true,
        "esModuleInterop": true,
        "jsx": "preserve",
        "jsxImportSource": "solid-js",
        "types": [
            "vite/client"
        ],
        "noEmit": true,
        "isolatedModules": true,
        "baseUrl": "src",
        "paths": {
            "#components/*": [
                "components/*"
            ],
            "#styles/*": [
                "styles/*"
            ],
            "#utilities/*": [
                "utilities/*"
            ],
            "#fetchers/*": [
                "fetchers/*"
            ],
            "#types/*": [
                "types/*"
            ],
            "#auth/*": [
                "auth/*"
            ]
        }
    }
}